{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Name as string,",
				"          age as integer,",
				"          experience as integer,",
				"          salary as float",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(salary>=2000) ~> filter1",
				"filter1 aggregate(groupBy(age,",
				"          experience),",
				"     total_sum = sum(salary)) ~> aggregate1"
			]
		}
	}
}