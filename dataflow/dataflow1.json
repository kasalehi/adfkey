{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          order_id as string,",
				"          customer_id as string,",
				"          order_status as string,",
				"          order_purchase_timestamp as string,",
				"          order_approved_at as string,",
				"          order_delivered_carrier_date as string,",
				"          order_delivered_customer_date as string,",
				"          order_estimated_delivery_date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(salary>=2000) ~> filter1",
				"filter1 aggregate(groupBy(age,",
				"          experience),",
				"     salary = avg(salary)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          AGE = age,",
				"          EXPERIENCE = experience,",
				"          SALARY = salary",
				"     )) ~> sink1"
			]
		}
	}
}