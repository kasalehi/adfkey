{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Name as string,",
				"          age as integer,",
				"          experience as integer,",
				"          salary as float",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(salary>=2000) ~> filter1",
				"filter1 aggregate(groupBy(age,",
				"          experience),",
				"     salary = avg(salary)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          NAME as string,",
				"          AGE as string,",
				"          EXPERIENCE as string,",
				"          SALARY as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          AGE = age,",
				"          EXPERIENCE = experience,",
				"          SALARY = salary",
				"     )) ~> sink1"
			]
		}
	}
}